{
    "name": "liste",
    "private": true,
    "type": "module",
    "version": "1.0.0",
    "description": "Template for Cloudflare pages using itty-router and svelte",
    "scripts": {
        "format": "prettier --write '**/*.{js,css,json,md}'",
        "dev": "wrangler pages dev ./public/ --persist-to=./.wrangler/state/d1 --d1=D1_lists --port=8888",
        "dev:all": "concurrently -n worker,pages -c blue,green \"npx wrangler dev --config=wrangler.worker.toml --port=8787\" \"npm run dev\"",
        "updateversion": "node setVersion.js",
        "build:front": "npm run updateversion && rollup -c",
        "build:auto": "npm run updateversion && rollup -c -w",
        "build:back": "tsc",
        "build": "npm run build:front && npm run build:back"
    },
    "author": "{{ authors }}",
    "license": "ISC",
    "devDependencies": {
        "concurrently": "^8.2.0",
        "@cloudflare/workers-types": "^4.20250722.0",
        "@smui/icon-button": "^6.0.0",
        "@smui/paper": "^6.0.0",
        "@svelte-plugins/tooltips": "^3.0.0",
        "@tsconfig/svelte": "^5.0.6",
        "prettier": "^1.17.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3"
    },
    "dependencies": {
        "@babel/preset-env": "^7.23.8",
        "@rollup/plugin-commonjs": "^24.0.0",
        "@rollup/plugin-node-resolve": "^15.0.0",
        "@rollup/plugin-terser": "^0.4.0",
        "@rollup/plugin-typescript": "^11.1.6",
        "CloudflareIttySvelteTemplate": "file:",
        "itty-router": "4.0.14",
        "lz-ts": "^1.1.2",
        "lzma-web": "^3.0.1",
        "material-design-inspired-color-picker": "^1.7.2",
        "rollup": "^3.15.0",
        "rollup-plugin-css-only": "^4.3.0",
        "rollup-plugin-livereload": "^2.0.0",
        "rollup-plugin-svelte": "^7.1.6",
        "serverless-cloudflare-workers": "^1.2.0",
        "svelte": "^4.0.0",
        "svelte-material-icons": "^3.0.5",
        "svelte-material-ui": "^6.2.0",
        "svelte-preprocess": "^5.1.3",
        "svelte-spa-router": "^3.3.0",
        "tslib": "^2.6.2"
    },
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "node"
    }
}
